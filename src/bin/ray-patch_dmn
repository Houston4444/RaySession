#!/usr/bin/env python3

import sys
from pathlib import Path
from typing import TYPE_CHECKING
src_path = Path(__file__).parent.parent
pyjacklib_path = src_path.parent / 'pyjacklib'
sys.path.insert(0, str(pyjacklib_path))
sys.path.insert(0, str(src_path))

sys.path[0] = str(Path(__file__).parents[1] / 'patchbay_daemon')

if TYPE_CHECKING:
    import src.patchbay_daemon.patchbay_daemon as pbd
    pbd.start()
else:
    import patchbay_daemon
    patchbay_daemon.start()
