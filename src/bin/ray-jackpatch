#!/usr/bin/env -S python3 -u

import sys
from pathlib import Path
from typing import TYPE_CHECKING

sys.path[0] = str(Path(__file__).parents[1] / 'clients')

if TYPE_CHECKING:
    from src.clients.jackpatch import jackpatch
    jackpatch.run()
else:
    import jackpatch.jackpatch as jackpatch
    jackpatch.run()
